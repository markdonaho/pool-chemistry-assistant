{
  "functions": [
    {
      "source": "functions-python",
      "codebase": "python-api",
      "runtime": "python312",
      "ignore": [
        "venv",
        ".git",
        "*.pyc",
        "__pycache__/"
      ]
    }
  ],
  "hosting": {
    "public": "frontend/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
